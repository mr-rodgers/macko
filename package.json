{
  "name": "@macko/macko",
  "version": "0.1.0",
  "description": "A simple websocket watcher",
  "main": "main.js",
  "scripts": {
    "build": "tsc -p . --pretty",
    "prestart": "npm run build",
    "watch": "concurrently --kill-others \"tsc --watch --pretty -p .\" \"npm run start\"",
    "start": "electron ."
  },
  "repository": "https://github.com/te-je/macko",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Te-je Rodgers",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^7.0.18",
    "concurrently": "^3.4.0",
    "electron": "~1.6.2",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "plugin-hooker": "^0.2.1"
  }
}
